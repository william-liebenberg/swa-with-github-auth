{
    "auth": {
        "identityProviders": {
            "github": {
                "registration": {
                    "clientIdSettingName": "GITHUB_CLIENT_ID",
                    "clientSecretSettingName": "GITHUB_CLIENT_SECRET"
                }
            }
        }
    },
    "routes": [
        {
            "route": "/weather*",
            "allowedRoles": ["authenticated"]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/api/*"
        ]
    }
}